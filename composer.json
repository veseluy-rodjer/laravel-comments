{
    "name": "klisl/laravel-comments",
    "description": "Package for using comments in Laravel-5",
	"type": "library",
	"keywords": [
        "laravel",
        "comments"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Klimenchuk",
            "email": "ksl80@ukr.net",
			"homepage": "https://github.com/klisl/laravel-comments",
			"role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/veseluy-rodjer/laravel-comments"
        }
    ],
	"minimum-stability": "stable",
	"require": {
	"klisl/laravel-comments": "dev",
        "php": ">=5.4.0"
    },
	"extra": {
      "laravel": {
        "providers": [
          "Klisl\\Comments\\CommentsServiceProvider"
        ]
      }
    },
    "autoload": {
        "psr-4": {
			"Klisl\\Comments\\": "src"
        }
    }
}
